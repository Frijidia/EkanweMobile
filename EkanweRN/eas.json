{
  "cli": {
    "version": ">= 6.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium",
        "buildConfiguration": "Release"
      },
      "cache": {
        "key": "no-cache-"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
